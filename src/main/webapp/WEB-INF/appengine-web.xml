<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="drivername" value="com.mysql.jdbc.Driver"/>
    <property name="googleusername" value="root"/>
    <property name="googlepassword" value="password"/>
    <property name="localusername" value="localusername"/>
    <property name="localpassword" value="password"/>
    <property name="databasename" value="dbname"/>
    
    <property name="socketfactory" value="com.google.cloud.sql.mysql.SocketFactory"/>
    <property name="cloudsqlinstance" value="instance-name"/>
    <property name="usessl" value="false"/>
  </system-properties>

</appengine-web-app>