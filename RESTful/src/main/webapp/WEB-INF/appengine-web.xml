<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>
  
  <automatic-scaling>
  	<max-instances>2</max-instances> 
  </automatic-scaling>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="drivername" value="com.mysql.jdbc.Driver"/>
    <property name="googleusername" value="root"/>
    <property name="googlepassword" value="salasana"/>
    <property name="localusername" value="bloodservice"/>
    <property name="localpassword" value="bloodservice"/>
    <property name="databasename" value="bloodservice"/>
    
    <property name="socketfactory" value="com.google.cloud.sql.mysql.SocketFactory"/>
    <property name="cloudsqlinstance" value="bloodtracker:europe-north1:blood-tracker"/>
    <property name="usessl" value="false"/>
  </system-properties>

</appengine-web-app>